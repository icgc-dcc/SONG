#!/bin/bash
docker build --build-arg SONG_VERSION=$SONG_VERSION --build-arg SONG_ARTIFACTORY_REPO_NAME=$SONG_ARTIFACTORY_REPO_NAME --build-arg SONG_VERSION_DATE=$SONG_VERSION_DATE --build-arg SONG_VERSION_SNAPSHOT=$SONG_VERSION_SNAPSHOT --build-arg SONG_ARTIFACTORY_REPO_NAME_SNAPSHOT=$SONG_ARTIFACTORY_REPO_NAME_SNAPSHOT -f $DOCKERFILE_PATH -t $IMAGE_NAME .
